#include <stdio.h>

int main()
{
	unsigned int a = 65529;
	signed int b = -5;

	if(b > a)
	{
		printf("\n yes \n");
	}


	if((unsigned int)b == ((4294967295-5+1) % 4294967296))
	{
		printf("\n true \n");
	}

	
	if((unsigned short int)b == ((65535-5+1) % 65535))
	{
		printf("\n true \n");
	}
	

	printf("%u",b);
	
	return 0;
}

